{% extends '::baseRompy.html.twig' %}

{% trans_default_domain 'templatesTranslations' %}

{% block stylesheets %}
	{{ parent() }}
	{% stylesheets 'bundles/frontendfrontoffice/css/*.css' filter='cssrewrite' %}
	    <link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/jquery.slick/1.3.13/slick.css"/>
{% endblock %}

{% block javascripts %}
	{{ parent() }}
    <script type="text/javascript" src="//cdn.jsdelivr.net/jquery.slick/1.3.13/slick.min.js"></script>
    {% javascripts '@FrontendFrontOfficeBundle/Resources/public/js/globals/*' %}
	    <script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}
{% endblock %}