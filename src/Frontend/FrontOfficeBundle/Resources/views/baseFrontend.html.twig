{% extends '::baseRompy.html.twig' %}

{% trans_default_domain 'templatesTranslations' %}

{% block stylesheets %}
	{{ parent() }}
	{% stylesheets 'bundles/frontendfrontoffice/css/*.css' filter='cssrewrite' %}
